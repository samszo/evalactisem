	<XmlParam nom="GetOntoTrad" datasource="http://localhost/evalactisem/library/CreaRdfTable.php">
		<Querys>
			<Query fonction="ieml-10eF">
				<select>SELECT t.ieml_id,t.10ef_id,i.ieml_lib,f.10ef_desc </select>
				<from> FROM ieml_trad t
					   INNER JOIN ieml_10ef f ON f.10ef_id = t.10ef_id 
					   INNER JOIN ieml_onto i ON i.ieml_id = t.ieml_id 
				</from>
				<where> </where>
			</Query>
		</Querys>
